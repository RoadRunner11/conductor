[program:conductor]
command=/srv/apps/conductor.pyz
    --workers 4
directory=/srv/apps
user=www-data
autostart=true
autorestart=true
stdout_logfile=/var/log/conductor.log
redirect_stderr=true
stopsignal=QUIT
environment=
{% for env_var, env_value in django_env|dictsort %}
    {{ env_var }}="{{ env_value }}",
{% endfor %}
